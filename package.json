{
  "name": "zsh-pyenv",
  "description": "Simple Python version management",
  "author": "Yuu Yamashita, Sam Stephenson, Mislav MarohniÄ‡, Josh Friend",
  "homepage": "https://github.com/pyenv/pyenv",
  "bugs": {
    "url": "https://github.com/pyenv/pyenv/issues"
  },
  "license": "MIT",
  "keywords": [
    "version management",
    "python version management",
    "zsh"
  ],
  "zsh-data": {
    "plugin-info": {
      "user": "pyenv",
      "plugin": "pyenv",
      "requires": "bgn;bash"
    },
    "zi-ices": {
      "default": {
        "git": "",
        "depth": 1,
        "lucid": "",
        "as": "command",
        "pick": "bin/pyenv",
        "atclone": "PYENV_ROOT=\"$PWD\" ./libexec/pyenv init - > zpyenv.zsh",
        "atpull": "%atclone",
        "atinit": "export PYENV_ROOT=\"$PWD\"",
        "src": "zpyenv.zsh",
        "nocompile": "!"
      },
      "bgn": {
        "requires": "bgn",
        "git": "",
        "lucid": "",
        "as": "null",
        "atclone": "PYENV_ROOT=\"$PWD\" ./libexec/pyenv init - > zpyenv.zsh",
        "atpull": "%atclone",
        "atinit": "export PYENV_ROOT=\"$PWD\"",
        "src": "zpyenv.zsh",
        "nocompile": "!",
        "sbin": "bin/pyenv"
      }
    }
  }
}
